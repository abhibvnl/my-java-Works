package ex3;

//rule 1
abstract class vehicle{
	abstract void start();
	
}

abstract class car extends vehicle{
	abstract void shiftGear();
	
}

class demo2 extends demo1{
  void test() {
	  System.out.println("hi");
  }
}
public class maincls {
	public static void main(String[] args) {
		
		demo2 obj =new demo2();
	    obj.test();
	}
}
