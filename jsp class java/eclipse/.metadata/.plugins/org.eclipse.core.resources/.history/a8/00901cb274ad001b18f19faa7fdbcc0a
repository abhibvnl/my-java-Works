		for (int i=0;i<wordArray.length;i++) {
			 wordArray[i]=word.charAt(i);
		}
		int cnt=0;
		int vowelsCnt=0;
		
		for (int i=0;i<wordArray.length;i++) {
		
			//System.out.println(cnt);
			/*if(wordArray[cnt]==vowels[i]) {
				vowelsCnt++;
			}*/
			 cnt++;
		}
		System.out.println(vowelsCnt);
		
		
