package upex3;

class demo1 {
	int x=10;
	void test(){
		System.out.println(" test() of demo1");
	}
}

class demo2 extends demo1 {
	int y=20;
	void disp(){
		System.out.println(" test() of demo1");
	}
}
class demo3 extends demo2 {
	int z=30;
	void view(){
		System.out.println(" test() of demo1");
	}
}
public class maincls {
 public static void main(String[] args) {
	demo1 obj1=  new demo3();
}
}
