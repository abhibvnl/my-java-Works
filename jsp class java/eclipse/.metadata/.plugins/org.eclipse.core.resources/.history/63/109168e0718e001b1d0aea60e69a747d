package inheritence3;


 class demo11 {

	int x=10;
	void disp1() {
		System.out.println(x);
	}
	
}

class demo22 extends demo11 {  // inheriting all  x and disp1() //it means all are copied into demo2 and u can use and access
	int y=10;
	
	demo22(){
		x=30;
	}
	
	void disp2() {
		System.out.println(y);
	}
	
}

public class maincls3 {

	public static void main(String[] args) {
		demo22 obj1=new demo22();
		System.out.println(obj1.x);
		//obj1.x=20;
		System.out.println(obj1.x);
		obj1.disp1();
	}

}




