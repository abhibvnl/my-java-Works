package calc;
import java.util.Scanner; 
interface cal{
	 
	  int add(int no1,int no2); //abs and pub
	  int sub(int no1,int no2);
	  double div(int no1,int no2);
	  int mul(int no1,int no2);	  
			  }
public class maincls implements cal{
	@Override
   public  int add(int no1,int no2)
   {
	   return no1+no2;
      }
	
	@Override
   public   int sub(int no1,int no2)
   {
    	 return no1-no2;
    }
	
	@Override
   public  double div(int no1,int no2){
    	 return no1/no2;
    }
	
	@Override
   public int mul(int no1,int no2){
    	return no1*no2;
    }
    public static void main(String[] args) {
		maincls m=new maincls();
		Scanner  scn=new Scanner(System.in);
		
		System.out.println("1:add\n2:sub\n3:mul\n4:div");
		m.add(0, 0);
		
	}

}