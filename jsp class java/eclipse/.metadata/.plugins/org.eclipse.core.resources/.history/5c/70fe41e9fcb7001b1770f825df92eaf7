package upperCaseLowerCase;

import java.util.Arrays;

public class prac {

	public static void main(String[] args) {
		
		String msg="abcdef";
		int values[]=new int[msg.length()];
		char characters[]=new char[msg.length()];
		
		for(int i=0;i<msg.length();i++) {
			int value=msg.charAt(i);
			values[i]=value-32;
			characters[i]=(char)values[i];
			String msg2=+Character.toString(values[i]);

		}
		String msg2=Arrays.toString(characters);
		System.out.println(Arrays.toString(values));
		System.out.println(Arrays.toString(characters));
		System.out.println(msg2);
	     
	
	
	
	}
}
