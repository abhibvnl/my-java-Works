package calc;
  interface cal{
	 
	  int add(int no1,int no2);
	  int sub(int no1,int no2);
	  double div(int no1,int no2);
	  int mul(int no1,int no2);
public class maincls extends calc
     int add(int no1,int no2){
	   return no1+no2;
      }
    int sub(int no1,int no2){
    	 return no1-no2;
    }
    
    double div(int no1,int no2){
    	 return no1/no2;
    }
{

}
