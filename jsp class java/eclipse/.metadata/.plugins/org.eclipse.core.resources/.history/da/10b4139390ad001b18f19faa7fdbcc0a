package prime;

public class prime {
	public static void main(String[] args) {
		
		 int num=4;
   		 int m=num/2;
		 boolean flag=false;
		 
		 if(num==1 || num==0 ){
			System.out.println("not prime");
		 }
		 else 
		 {
			for(int i=2; i<=m; i++) {
				if(num%i==0) {
				  System.out.println("not prime");
				  flag=true;
				}
			}
		 }
		 
		 if(flag==false)
			 System.out.println(num+ " is prime no");
	}

 
 
}
