package proj2;

public class demo1 {
	
	private int var1=20;    // access lvl only within  class
	        int var2=20;    //default access specifier is package ,access lvl only within  package
	protected int var3=20;  // access lvl only within  package and outside with is a relation
	public int var4=20;    //access lvl   everywhere
	
   void	f1() {
	   
   }

}
